<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>ConfirmacionOferta</servlet-name>
        <servlet-class>co.edu.udea.PInscriptionSystem.Controlador.ConfirmacionOferta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConfirmacionOferta</servlet-name>
        <url-pattern>/ConfOferta</url-pattern>
    </servlet-mapping>
        <servlet>
        <servlet-name>LobbyAdmin</servlet-name>
        <servlet-class>co.edu.udea.PInscriptionSystem.Controlador.LobbyAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LobbyAdmin</servlet-name>
        <url-pattern>/lobbyAdm</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>